{
  "name": "nestjs-graphql-prisma",
  "version": "0.0.1",
  "description": "a template of nestjs with graphql and prisma",
  "author": "Cookie_gg",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "cleanup": "dotenv -e .env.development -- prisma migrate reset --force",
    "start": "NODE_ENV=\"development\" nest start",
    "start:dev": "NODE_ENV=\"development\" nest start --watch",
    "start:debug": "NODE_ENV=\"development\" nest start --debug --watch",
    "start:prod": "NODE_ENV=\"production\" node dist/main",
    "prisma:docs": "prisma-docs-generator serve",
    "prisma:studio": "prisma studio",
    "prisma:dev": "run-s prisma:prismixer prisma:generate:dev prisma:format prisma:migrate:dev",
    "prisma:test": "run-s prisma:prismixer prisma:generate:test prisma:format prisma:migrate:test",
    "prisma:prismixer": "rimraf ./prisma/schema.prisma && prismixer",
    "prisma:format": "prisma format",
    "prisma:generate:dev": "dotenv -e .env.development -- prisma generate",
    "prisma:generate:test": "dotenv -e .env.test -- prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev --name init",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate dev --name test",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "run-s test:cleanup && dotenv -e .env.test -- jest",
    "test:cleanup": "dotenv -e .env.test -- prisma migrate reset --force",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "run-s test:cleanup && dotenv -e .env.test -- jest --config jest-e2e.config.js"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.0.9",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.4.4",
    "@nestjs/graphql": "^10.0.10",
    "@nestjs/platform-express": "^8.4.4",
    "@nestjs/platform-fastify": "^8.4.4",
    "@prisma/client": "^3.13.0",
    "apollo-server-express": "^3.6.7",
    "apollo-server-fastify": "^3.6.7",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^16.4.0",
    "graphql-type-json": "^0.3.2",
    "prisma-docs-generator": "^0.5.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5"
  },
  "devDependencies": {
    "@jagi/jest-transform-graphql": "^1.0.2",
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.10",
    "@nestjs/testing": "^8.4.4",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.4.1",
    "@types/node": "^17.0.30",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.2.5",
    "jest-mock-extended": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "prisma-nestjs-graphql": "^15.2.2",
    "prismixer": "^1.0.4",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.9",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.4"
  },
  "volta": {
    "node": "16.15.0",
    "npm": "8.8.0"
  }
}
